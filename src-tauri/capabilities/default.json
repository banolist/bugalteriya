{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "fs:default",
    "fs:allow-app-write",
    "fs:allow-app-write-recursive",
    "fs:allow-appcache-write",
    "fs:allow-appcache-write-recursive",
    "dialog:default"
  ]
}
